{
  "name": "node-clonefile",
  "version": "0.0.1",
  "description": "Darwin clonefile api for node",
  "keywords": [
    "clonefile",
    "darwin",
    "macos",
    "BSD"
  ],
  "main": "dist/index.js",
  "bin": {
    "clonefile": "dist/cli.js"
  },
  "os": ["darwin"],
  "gypfile": true,
  "license": "MIT",
  "files": [
    "binding.gyp",
    "src/darwin/",
    "dist/"
  ],
  "scripts": {
    "build": "node-gyp build && tsc && npm run pretty",
    "pretty": "prettier --write binding.gyp --parser json && prettier --write src/**/*.ts dist/**/*.js dist/**/*.ts example/**/* && clang-format -i src/darwin/*",
    "lint": "eslint src"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^8.2.1"
  },
  "devDependencies": {
    "@eslint/js": "9.13.0",
    "@types/eslint__js": "8.42.3",
    "@types/node": "^22.7.7",
    "@typescript-eslint/parser": "8.10.0",
    "eslint": "9.13.0",
    "typescript": "5.6.3",
    "typescript-eslint": "8.10.0"
  }
}
